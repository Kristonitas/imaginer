  },
  "homepage": "https://github.com/Kristonitas/imaginer#readme",
  "dependencies": {
    "canvas": "^2.6.1",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@types/express": "4.17.9",
    "ts-node-dev": "1.1.1",
    "typescript": "4.1.3"
  }
